{
  "family": "hecate",
  "requiresCompatibilities": ["EC2"],
  "networkMode": "bridge",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::058264371750:role/nullblock-ecs-task-execution",
  "taskRoleArn": "arn:aws:iam::058264371750:role/nullblock-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "hecate",
      "image": "ghcr.io/aetherbytes/nullblock/hecate:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/ghcr-token-UmAOoh"
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "wget --spider -q http://localhost:3000/ || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/nullblock/hecate",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {"name": "VITE_API_URL", "value": "https://nullblock.io"},
        {"name": "NODE_ENV", "value": "production"},
        {"name": "PORT", "value": "5173"}
      ],
      "secrets": [],
      "mountPoints": [],
      "volumesFrom": []
    }
  ],
  "volumes": []
}
