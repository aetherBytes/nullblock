{
  "family": "nullblock-engrams",
  "requiresCompatibilities": ["EC2"],
  "networkMode": "bridge",
  "memory": "256",
  "cpu": "128",
  "executionRoleArn": "arn:aws:iam::058264371750:role/nullblock-ecs-task-execution",
  "taskRoleArn": "arn:aws:iam::058264371750:role/nullblock-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "nullblock-engrams",
      "image": "ghcr.io/aetherbytes/nullblock/nullblock-engrams:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9004,
          "hostPort": 9004,
          "protocol": "tcp"
        }
      ],
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/ghcr-token-UmAOoh"
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:9004/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/nullblock/engrams",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {"name": "RUST_LOG", "value": "info"},
        {"name": "HOST", "value": "0.0.0.0"},
        {"name": "PORT", "value": "9004"}
      ],
      "secrets": [
        {"name": "DATABASE_URL", "valueFrom": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/prod-5afB92:DB_URL_EREBUS::"}
      ],
      "mountPoints": [],
      "volumesFrom": []
    }
  ],
  "volumes": []
}
