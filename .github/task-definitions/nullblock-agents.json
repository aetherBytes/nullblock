{
  "family": "nullblock-agents",
  "requiresCompatibilities": ["EC2"],
  "networkMode": "bridge",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::058264371750:role/nullblock-ecs-task-execution",
  "taskRoleArn": "arn:aws:iam::058264371750:role/nullblock-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "nullblock-agents",
      "image": "ghcr.io/aetherbytes/nullblock/nullblock-agents:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 9003,
          "hostPort": 9003,
          "protocol": "tcp"
        }
      ],
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/ghcr-token-UmAOoh"
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:9003/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/nullblock/agents",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {"name": "RUST_LOG", "value": "info"},
        {"name": "HOST", "value": "0.0.0.0"},
        {"name": "PORT", "value": "9003"},
        {"name": "ENGRAMS_SERVICE_URL", "value": "http://172.17.0.1:9004"},
        {"name": "EREBUS_BASE_URL", "value": "https://nullblock.io"}
      ],
      "secrets": [
        {"name": "DATABASE_URL", "valueFrom": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/prod-5afB92:DB_URL_AGENTS::"},
        {"name": "OPENROUTER_KEY_HECATE", "valueFrom": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/prod-5afB92:OPENROUTER_KEY_HECATE::"},
        {"name": "OPENROUTER_KEY_MOROS", "valueFrom": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/prod-5afB92:OPENROUTER_KEY_MOROS::"},
        {"name": "OPENROUTER_KEY_CLAWROS", "valueFrom": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/prod-5afB92:OPENROUTER_KEY_CLAWROS::"},
        {"name": "ENCRYPTION_MASTER_KEY", "valueFrom": "arn:aws:secretsmanager:us-west-2:058264371750:secret:nullblock/prod-5afB92:ENCRYPTION_MASTER_KEY::"}
      ],
      "mountPoints": [],
      "volumesFrom": []
    }
  ],
  "volumes": []
}
